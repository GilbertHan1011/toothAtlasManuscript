@article{krivanekDentalCellType2020b,
  title = {Dental Cell Type Atlas Reveals Stem and Differentiated Cell Types in Mouse and Human Teeth},
  author = {Krivanek, Jan and Soldatov, Ruslan A. and Kastriti, Maria Eleni and Chontorotzea, Tatiana and Herdina, Anna Nele and Petersen, Julian and Szarowska, Bara and Landova, Marie and Matejova, Veronika Kovar and Holla, Lydie Izakovicova and Kuchler, Ulrike and Zdrilic, Ivana Vidovic and Vijaykumar, Anushree and Balic, Anamaria and Marangoni, Pauline and Klein, Ophir D. and Neves, Vitor C. M. and Yianni, Val and Sharpe, Paul T. and Harkany, Tibor and Metscher, Brian D. and Baj{\'e}noff, Marc and Mina, Mina and Fried, Kaj and Kharchenko, Peter V. and Adameyko, Igor},
