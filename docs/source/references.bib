  langid = {english},
  keywords = {Data integration,Gene expression,Software},
  file = {/home/gilberthan/Zotero/storage/UWSF8N4T/Büttner 等 - 2019 - A test metric for assessing single-cell RNA-seq ba.pdf;/home/gilberthan/Zotero/storage/XIBVL55P/Büttner 等 - 2019 - A test metric for assessing single-cell RNA-seq ba.pdf}
}

@article{fresiaBiteByteDental2021,
  title = {From {{Bite}} to {{Byte}}: {{Dental Structures Resolved}} at a {{Single-Cell Resolution}}},
  shorttitle = {From {{Bite}} to {{Byte}}},
  author = {Fresia, R. and Marangoni, P. and {Burstyn-Cohen}, T. and Sharir, A.},
  year = {2021},
  month = aug,
  journal = {Journal of Dental Research},
  volume = {100},
  number = {9},
  pages = {897--905},
  publisher = {SAGE Publications Inc},
  issn = {0022-0345},
  doi = {10.1177/00220345211001848},
  urldate = {2023-04-29},
  abstract = {The systematic classification of the cells that compose a tissue or an organ is key to understanding how these cells cooperate and interact as a functional unit. Our capacity to detect features that define cell identity has evolved from morphological and chemical analyses, through the use of predefined genetic markers, to unbiased transcriptomic and epigenetic profiling. The innovative technology of single-cell RNA sequencing (scRNA-seq) enables transcriptional profiling of thousands of individual cells. Since its development, scRNA-seq has been extensively applied to numerous organs and tissues in a wide range of animal models and human samples, thereby providing a plethora of fundamental biological insights into their development, homeostasis, and pathology. In this review, we present the findings of 3 recent studies that employed scRNA-seq to unravel the complexity of cellular composition in mammalian teeth. These findings offer an unprecedented catalogue of cell types in the mouse incisor, which is a convenient model system for studying continuous tooth growth. These studies identified novel cell types in the tooth epithelium and mesenchyme, as well as new markers for known cell types. Computational analyses of the data also uncovered the lineage and dynamics of cell states during ameloblast and odontoblast differentiation during both normal homeostasis and injury repair. The transcriptional differences between the mouse incisor and mouse and human molars uncover species-specific as well as shared features in tooth cell composition. Here, we highlight these findings and discuss important similarities and differences between these studies. We also discuss potential future applications of scRNA-seq in dental research and dentistry. Together, these studies demonstrate how the rapidly evolving technology of scRNA-seq can advance the study of tooth development and function and provide putative targets for regenerative approaches.},
  langid = {english},
  keywords = {/unread}
}

@article{wangSingleCellAtlas2022,
  title = {Single Cell Atlas of Developing Mouse Dental Germs Reveals Populations of {{CD24}}+ and {{Plac8}}+ Odontogenic Cells},
  author = {Wang, Yaofeng and Zhao, Yifan and Chen, Shubin and Chen, Xiaoming and Zhang, Yanmei and Chen, Hong and Liao, Yuansong and Zhang, Jiashu and Wu, Di and Chu, Hongxing and Huang, Hongying and Wu, Caixia and Huang, Shijuan and Xu, Huichao and Jia, Bei and Liu, Jie and Feng, Bo and Li, Zhonghan and Qin, Dajiang and Pei, Duanqing and Cai, Jinglei},
  year = {2022},
